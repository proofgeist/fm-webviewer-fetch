  "dependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.6",
    "@proofgeist/fmdapi": "4.0.0-beta.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
